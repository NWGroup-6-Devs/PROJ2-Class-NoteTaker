<section id="gigs" class="container">
    <h1>All NOTES</h1>

    {{#each notes}}
    <div class="gig">
        <h3>{{dataValues.title}}</h3>
        <p>{{dataValues.description}}</p>
        <ul>
            <li><a href="mailto:{{dataValues.contact_email}}" class="btn btn-reverse">Send note via E-mail</a></li>
        </ul>
    </div>
    {{else}}
    <p>No Note available</p>
    {{/each}}

</section>